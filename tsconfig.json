{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/": ["./src"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@styles/*": ["./src/styles/"],
      "@types/*": ["./src/types/*"],
      "@utils": ["./src/utils"],
    }
  }
}